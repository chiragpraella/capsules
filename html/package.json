{
  "name": "tailwind-gsap-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel watch serve",
    "watch": "postcss src/styles/tailwind.css -o public/styles/tailwind.css --watch",
    "build": "postcss src/styles/tailwind.css -o public/styles/tailwind.css --env production",
    "serve": "http-server ./ -c-1 -p 5173"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.13",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5"
  }
}
